{
   "author": "pmasta.tr@gmail.com",
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "img/btn/icon128.png",
      "default_title": "Agar.io"
   },
   "content_scripts": [ {
      "js": [ "inject.js" ],
      "matches": [ "*://agar.io/", "*://agar.io/index.html", "*://agar.io/?ip=*" ],
      "run_at": "document_start"
   } ],
   "description": "Minions and hacks for Agar.io by Eren Kara!",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Agar.io Minions by Eren Kara",
   "permissions": [ "*://agar.io/*", "webRequest", "webRequestBlocking" ],
   "short_name": "Agar.io++",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.1",
   "web_accessible_resources": [ "html/*", "img/*" ]
}
